    "from __future__ import unicode_literals\n",
    "from pyecharts import Bar\n",
    "from pyecharts import online\n",
    "\n",
    "online() # 使用远程 jshost\n",
    "bar = Bar(\"我的第一个图表\", \"这里是副标题\")\n",
    "bar.use_theme('dark')\n",
    "bar.add(\"服装\", [\"衬衫\", \"羊毛衫\", \"雪纺衫\", \"裤子\", \"高跟鞋\", \"袜子\"], [5, 20, 36, 10, 75, 90])\n",
    "bar.render()\n",
    "bar"
   ]
